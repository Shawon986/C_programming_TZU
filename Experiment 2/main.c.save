#include <stdio.h>
#include <math.h>

int main() {
    // Declare variables for coefficients
    float a, b, c;

    // Prompt the user to enter coefficients
    printf("Enter the value of a: ");
    scanf("%f", &a);

    printf("Enter the value of b: ");
    scanf("%f", &b);

    printf("Enter the value of c: ");
    scanf("%f", &c);

    // Calculate the discriminant
    float delta = b * b - 4 * a * c;

    // Check if the roots are real
    if (delta >= 0) {
        // Calculate the two roots
        float x1 = (-b + sqrt(delta)) / (2 * a);
        float x2 = (-b - sqrt(delta)) / (2 * a);

        // Print the roots in the required format with two decimal places
        printf("%.2f %.2f\n", fmax(x1, x2), fmin(x1, x2));
    } else {
        // If the discriminant is negative, roots are complex
        printf("The roots are complex and cannot be represented in this format.\n");
    }

    return 0;
}
